package testCases;

import org.openqa.selenium.chrome.ChromeDriver;

public class ChromeTestBrowser {

	public static void main(String arg[]){
		
		System.setProperty("webdriver.chrome.driver", "D:\\ABHISHEK\\Udemy\\selenium\\executables\\chromedriver.exe");
		
		ChromeDriver driver = new ChromeDriver();
		
		driver.get("https://chrome.google.com/webstore/category/extensions?utm_source=chrome-ntp-icon");
	}
}
